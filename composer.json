{
    "name": "yousefkadah/freepbx-laravel-integrator",
    "description": "A comprehensive Laravel package for integrating FreePBX telephony features into SaaS CRM applications with full multi-tenant support",
    "keywords": [
        "laravel",
        "freepbx",
        "asterisk",
        "ami",
        "telephony",
        "voip",
        "multi-tenant",
        "crm"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yousef Kadah",
            "email": "yousefkadah@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/broadcasting": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.5",
        "react/socket": "^1.14",
        "react/event-loop": "^1.4",
        "react/promise": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0",
        "laravel/pint": "^1.0",
        "phpstan/phpstan": "^1.10",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "yousefkadah\\FreePbx\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yousefkadah\\FreePbx\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "yousefkadah\\FreePbx\\FreePbxServiceProvider"
            ],
            "aliases": {
                "FreePbx": "yousefkadah\\FreePbx\\Facades\\FreePbx"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
